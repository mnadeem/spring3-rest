<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
	<configuration>
		<stop-on-errors>true</stop-on-errors>
		<date-format>MM/dd/yyyy HH:mm</date-format>
		<wildcard>true</wildcard>
	</configuration>
	
	<mapping>
		<class-a>com.nadeem.app.rest.repository.entitiy.Employee</class-a>
		<class-b>com.nadeem.app.rest.web.support.data.EmployeeData</class-b>
		<field>
			<a>id</a>
			<b>id</b>
		</field>
		<field>
			<a>name</a>
			<b>name</b>
		</field>
		<field>
			<a>email</a>
			<b>email</b>
		</field>
		<field>
			<a>creationTime</a>
			<b>creationTime</b>
		</field>
		<field>
			<a>modificationTime</a>
			<b>modificationTime</b>
		</field>	
	</mapping>

</mappings>  